[case testIntBaseFormatConstantFold]
from typing import Final

FMT_X: Final = "{:x}"
FMT_XU: Final = "{:X}"
FMT_O: Final = "{:o}"
FMT_B: Final = "{:b}"

def hx() -> str:
    return FMT_X.format(255)

def hx_u() -> str:
    return FMT_XU.format(255)

def ho() -> str:
    return FMT_O.format(255)

def hb() -> str:
    return FMT_B.format(10)
